    "import turtle as t\n",
    "\n",
    "def sq(x):\n",
    "    for i in range(4):\n",
    "        joe.color(colors[i])\n",
    "        joe.forward(x)\n",
    "        joe.left(90)\n",
    "\n",
    "colors = ['blue', 'green', 'orange', 'pink']\n",
    "\n",
    "joe = t.Turtle()\n",
    "joe.speed(50)\n",
    "\n",
    "dlina = 30\n",
    "for i in range(50):\n",
    "    sq(dlina)\n",
    "    joe.left(15)\n",
    "    dlina += 5"
   ]
